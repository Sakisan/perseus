  -moz-transition: opts
  -ms-transition: opts
  -o-transition: opts
  transition: opts

round-corners(opts)
  -webkit-border-radius: opts
  -moz-border-radius: opts
  -ms-border-radius: opts
  -o-border-radius: opts
  border-radius: opts

animation-name(name)
